# -------------------------
# BINANCE CONFIGURATION
# -------------------------

# Trading pair symbol (e.g., BTCUSDT, ETHUSDT)
SYMBOL=EXAMPLE_SYMBOL

# Time interval for data fetching (e.g., 1m, 5m, 15m, 1h)
# full list here: https://binance-docs.github.io/apidocs/spot/en/#kline-candlestick-streams
TIMEFRAME=EXAMPLE_TIMEFRAME

# Maximum number of records to retrieve (e.g., 50, 100, 500)
LIMIT=EXAMPLE_LIMIT_NUMBER


# -------------------------
# TELEGRAM CONFIGURATION
# -------------------------

# Telegram bot token for notifications (Format: 1111111111:AAABBBCCCDDD-EEE)
TELEGRAM_BOT_TOKEN=EXAMPLE_TELEGRAM_BOT_TOKEN

# Chat ID for sending Telegram notifications (e.g., 123456789)
TELEGRAM_CHAT_ID=EXAMPLE_TELEGRAM_CHAT_ID


# -------------------------
# TRADING STRATEGY CONFIGURATION
# -------------------------

# Stop loss percentage (from entry price)
STOP_LOSS_PERCENT=1.0

# Take profit percentages (from entry price)
TAKE_PROFIT_1_PERCENT=2.0
TAKE_PROFIT_2_PERCENT=3.0
TAKE_PROFIT_3_PERCENT=6.0

# Number of decimal places for rounding
ROUND_DECIMAL_PLACES=2
